<div>
  <ol style="display: flex; flex-wrap:wrap">
    {{#each elementos}}
      <div class="card" style="width: 18rem;margin:2rem">
        <img src={{this.thumbnail}} class="card-img-top" alt="..."/>
        <div class="card-body">
          <h5 class="card-title">{{this.title}}</h5>
          <p class="card-text">{{this.description}}</p>
          
        </div>
      </div>

      
    {{/each}}
  </ol>
</div>